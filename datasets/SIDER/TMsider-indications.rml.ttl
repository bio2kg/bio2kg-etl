@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix sidv: <http://bio2rdf.org/sider_vocabulary:> .
@prefix stitchv: <http://bio2rdf.org/stitch_vocabulary:> .
@prefix meddrav: <http://bio2rdf.org/meddra_vocabulary:> .

<TMsider-indications1>

	rml:logicalSource [
		rml:source "http://sideeffects.embl.de/media/download/meddra_all_indications.tsv.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class sidv:Resource;
		rr:class sidv:Drug-Indication-Association;
		rr:template "http://bio2rdf.org/sider:{STITCH ID flat}_{UMLS ID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "stitch:{STITCH ID flat} - meddra:{UMLS ID} [sider:{STITCH ID flat}_{UMLS ID}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "stitch:{STITCH ID flat} - meddra:{UMLS ID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "sider:{STITCH ID flat}_{UMLS ID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant sidv:drug ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/stitch:{STITCH ID flat}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant sidv:indication ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/meddra:{UMLS ID}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant sidv:provenance ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Detection method"; rr:datatype xsd:string; ];
	];

.

