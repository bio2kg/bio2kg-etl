@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix taxv: <http://bio2rdf.org/taxonomy_vocabulary:> .

<TMtaxonomy-names1>

	rml:logicalSource [
		rml:source "ftp://ftp.ncbi.nih.gov/pub/taxonomy/taxdmp.zip/names.dmp" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class taxv:Resource;
		rr:template "http://bio2rdf.org/taxonomy:{tax_id}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[
			fnml:functionValue [
				rr:predicateObjectMap [
					rr:predicateMap fno:executes ;
					rr:objectMap [ rr:constant sql:concat ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam ;
					rr:objectMap [ rr:constant "taxv:" ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam2 ;
					rr:objectMap	[
						fnml:functionValue [
							rr:predicateObjectMap [
								rr:predicateMap fno:executes ;
								rr:objectMap [ rr:constant sql:replace ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam ;
								rr:objectMap [ rml:reference "name_class" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam2 ;
								rr:objectMap [ rr:constant " " ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam3 ;
								rr:objectMap [ rr:constant "-" ];
							];
						];
					];
				];
			];
		];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "name_txt"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant taxv:unique-name ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "tax_id"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "taxonomy:{tax_id}"; rr:datatype xsd:string; ];
	];

.

