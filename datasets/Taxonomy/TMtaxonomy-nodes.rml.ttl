<TMtaxonomy-nodes1>

	rml:logicalSource [
		rml:source "ftp://ftp.ncbi.nih.gov/pub/taxonomy/taxdmp.zip/nodes.dmp" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class taxv:Resource;
		rr:template "http://bio2rdf.org/taxonomy:{tax_id}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:subClassOf ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/taxonomy:{parent_tax_id}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant taxv:rank ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/taxonomy_resource:{rank}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant taxv:embl-code ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/taxonomy_resource:{embl-code}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant taxv:division-id ];
		rr:objectMap	[
			rr:parentTriplesMap <TMtaxonomy-division1>;
			rr:joinCondition [ rr:child "division id"; rr:parent "division_id"; ];
		];
	];

