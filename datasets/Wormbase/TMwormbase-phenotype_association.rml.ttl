@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix wbv: <http://bio2rdf.org/wormbase_vocabulary:> .

<TMwormbase-phenotype_association1>

	rml:logicalSource [
		rml:source "ftp://ftp.wormbase.org/pub/wormbase/releases/current-production-release/ONTOLOGY/phenotype_association.WS269.wb" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class wbv:Resource;
		rr:template "http://bio2rdf.org/wormbase_resource:{gene_id}_{phenotype}_{variant}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "gene-phenotype association between {gene_id} and {phenotype} under condition {variant} [wormbase_resource:{gene_id}_{phenotype}_{variant}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "gene-phenotype association between {gene_id} and {phenotype} under condition {variant}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "wormbase_resource:{gene_id}_{phenotype}_{variant}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant wbv:phenotype ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "phenotype"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant wbv:gene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMwormbase-geneid>;
			rr:joinCondition [ rr:child "gene_id"; rr:parent "gene_id"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant wbv:evidence-type ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "evidence"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant wbv:condition ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "variant"; rr:datatype xsd:string; ];
	];

.

