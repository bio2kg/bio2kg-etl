@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix hgncv: <http://bio2rdf.org/hgnc_vocabulary:> .
@prefix hsv: <http://bio2rdf.org/hgnc.symbol_vocabulary:> .

<TMhgnc1>

	rml:logicalSource [
		rml:source "ftp://ftp.ebi.ac.uk/pub/databases/genenames/hgnc_complete_set.txt.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class hgncv:Resource;
		rr:class hgncv:Gene-Symbol;
		rr:template "http://bio2rdf.org/{HGNC_ID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Gene Symbol for {Approved_Symbol} [{HGNC_ID}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Gene Symbol for {Approved_Symbol}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "{HGNC_ID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:approved-symbol ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Approved_Symbol"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:has-approved-symbol ];
		rr:objectMap	[
			rr:parentTriplesMap <TMhgnc2>;
			rr:joinCondition [ rr:child "Approved_Symbol"; rr:parent "Approved_Symbol"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:approved-name ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Approved_Name"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:status ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "hgncv:{Status}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:locus ];
		rr:objectMap	[
			rr:parentTriplesMap <TMhgnc3>;
			rr:joinCondition [ rr:child "HGNC ID"; rr:parent "HGNC_ID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:previous-symbol ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Previous_Symbols"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:previous-name ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Previous_Names"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:name-synonym ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Name_Synonyms"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:synonym ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Synonyms"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:chromosome ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Chromosome"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:date-approved ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Date_Approved"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:date-modified ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Date_Modified"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:date-symbol-changed ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Date_Symbol_Changed"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:date-name-changed ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Date_Name_Changed"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:accession ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Accession_Numbers"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ec ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Enzyme_IDs"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ncbigene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info>;
			rr:joinCondition [ rr:child "Entrez_Gene_ID"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ensembl ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://rdf.ebi.ac.uk/resource/ensembl/{Ensembl_Gene_ID}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-mgi ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Mouse_Genome_Database_ID"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:xref ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Specialist_Database_Links"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-pubmed ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pubmed:[{Pubmed_IDs}]"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-refseq ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/refseq:{RefSeq_IDs}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:gene-family-tag ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Gene_Family_Tag"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:gene-family-description ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Gene_family_description"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:record-type ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Record_Type"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:primary-id ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Primary_IDs"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:secondary-id ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Secondary_IDs"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ccds ];
		rr:objectMap	[ rr:termType rr:IRI; rml:reference "http://bio2rdf.org/ccds:{CCDS_IDs}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-vega ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/vega:{VEGA_IDs}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:locus-specific-xref ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Locus_Specific_Databases"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ncbigene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info>;
			rr:joinCondition [ rr:child "Entrez_Gene_ID_(supplied_by_NCBI)"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-omim ];
		rr:objectMap	[ rr:termType rr:IRI; rml:reference "http://bio2rdf.org/omim:{OMIM_ID_(supplied_by_NCBI)}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-refseq ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/refseq:{RefSeq_(supplied_by_NCBI)}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-uniprot ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/uniprot:{UniProt_ID_(supplied_by_UniProt)}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ensembl ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://rdf.ebi.ac.uk/resource/ensembl/{Ensembl_ID_(supplied_by_Ensembl)}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-vega ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/vega:{Vega_ID_(supplied_by_Vega)}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-ucsc ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/ucsc:{UCSC_ID_(supplied_by_UCSC)}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-mgi ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Mouse_Genome_Database_ID_(supplied_by_MGI)"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:x-rgd ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Rat_Genome_Database_ID_(supplied_by_RGD)"; rr:datatype xsd:string; ];
	];

.


<TMhgnc2>

	rml:logicalSource [
		rml:source "ftp://ftp.ebi.ac.uk/pub/databases/genenames/hgnc_complete_set.txt.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class hgncv:Approved-Gene-Symbol;
		rr:class hsv:Resource;
		rr:template "http://bio2rdf.org/hgnc.symbol:{Approved_Symbol}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "{Approved_Symbol} [hgnc.symbol:{Approved Symbol}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Approved_Symbol"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "hgnc.symbol:{Approved_Symbol}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:is-approved-symbol-of ];
		rr:objectMap	[
			rr:parentTriplesMap <TMhgnc1>;
			rr:joinCondition [ rr:child "Approved Symbol"; rr:parent "Approved_Symbol"; ];
		];
	];

.


<TMhgnc3>

	rml:logicalSource [
		rml:source "ftp://ftp.ebi.ac.uk/pub/databases/genenames/hgnc_complete_set.txt.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class hgncv:Locus;
		rr:template "http://bio2rdf.org/{HGNC_ID}_LOCUS";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:locus-type ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Locus_Type"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant hgncv:locus-group ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "Locus_Group"; rr:datatype xsd:string; ];
	];

.

