@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix genagev: <http://bio2rdf.org/genage_vocabulary:> .

<TMgenage_model1>

	rml:logicalSource [
		rml:source "http://genomics.senescence.info/genes/models_genes.zip" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class genagev:Resource;
		rr:class genagev:Aging-Related-Gene;
		rr:template "http://bio2rdf.org/genage:{GenAge_ID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Aging related gene {GenAge_ID} [genage-model:{GenAge_ID}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Aging related gene {GenAge_ID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "[genage-model:{GenAge_ID}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant genagev:gene-symbol ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "symbol"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant genagev:taxon ];
		rr:objectMap	[
			rr:parentTriplesMap <TMtaxonomy-names1>;
			rr:joinCondition [ rr:child "organism"; rr:parent "name_txt"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant genagev:x-ncbigene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info1>;
			rr:joinCondition [ rr:child "entrez_gene_id"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant genagev:maximum-percent-observed-average-lifespan-change ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "avg_lifespan_change_(max_obsv)"; rr:datatype xsd:float; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant genagev:lifespan-effect ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "lifespan_effect"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant genagev:longevity-influence ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "longevity_influence"; rr:datatype xsd:string; ];
	];

.

