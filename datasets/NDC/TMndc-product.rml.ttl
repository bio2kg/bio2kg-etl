@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix ndcv: <http://bio2rdf.org/ndc_vocabulary:> .

<TMndc-product1>

	rml:logicalSource [
		rml:source "https://www.accessdata.fda.gov/cder/ndctext.zip~product.txt" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ndcv:Resource;
		rr:class ndcv:Product;
		rr:template "http://bio2rdf.org/ndc:{PRODUCTID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "{PROPRIETARYNAME} [ndc:{PRODUCTID}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "PROPRIETARYNAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "ndc:{PRODUCTID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:has-package ];
		rr:objectMap	[
			rr:parentTriplesMap <TMndc-package1>;
			rr:joinCondition [ rr:child "PRODUCTID"; rr:parent "PRODUCTID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:product-type ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "PRODUCTTYPENAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:product-id ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "PRODUCTNDC"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:proprietary-name ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "PROPRIETARYNAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:trade-name-suffix ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "PROPRIETARYNAMESUFFIX"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:non-proprietary-anem ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "NONPROPRIETARY"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:dosage-form ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "DOSAGEFORMNAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:route ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "ROUTENAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:start-marketing-date ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "STARTMARKETINGDATE"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:end-marketing-date ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "ENDMARKETINGDATE"; rr:datatype xsd:date; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:marketing-category ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "MARKETINGCATEGORYNAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:application-number ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "APPLICATIONNUMBER"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:labeller ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "LABELERNAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:pharmacological-class ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "PHARM_CLASSES"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:ingredient ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "SUBSTANCENAME"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:has-part ];
		rr:objectMap	[
			rr:parentTriplesMap <TMndc-product2>;
			rr:joinCondition [ rr:child "{ACTIVE_NUMERATOR_STRENGTH}{ACTIVE_INGRED_UNIT} {SUBSTANCENAME}"; rr:parent "{ACTIVE_NUMERATOR_STRENGTH}{ACTIVE_INGRED_UNIT} {SUBSTANCENAME}"; ];
		];
	];

.


<TMndc-product2>

	rml:logicalSource [
		rml:source "https://www.accessdata.fda.gov/cder/ndctext.zip~product.txt" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ndcv:Resource;
		rr:template "http://bio2rdf.org/ndc_resource:{PRODUCTNDC}_{SUBSTANCENAME}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "{ACTIVE_NUMERATOR_STRENGTH}{ACTIVE_INGRED_UNIT} {SUBSTANCENAME} [ndc_resource:{PRODUCTNDC}_{SUBSTANCENAME}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "{ACTIVE_NUMERATOR_STRENGTH}{ACTIVE_INGRED_UNIT} {SUBSTANCENAME}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "ndc_resource:{PRODUCTNDC}_{SUBSTANCENAME}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:amount ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "ACTIVE_NUMERATOR_STRENGTH"; rr:datatype xsd:float; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ndcv:amount_unit ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "ACTIVE_INGRED_UNIT"; rr:datatype xsd:string; ];
	];

.

