@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix ipcv: <http://bio2rdf.org/iproclass_vocabulary:> .

<TMiproclass1>

	rml:logicalSource [
		rml:source "ftp://ftp.pir.georgetown.edu/databases/iproclass/iproclass.tb.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ipcv:Resource;
		rr:template "http://bio2rdf.org/iproclass:{hid}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "iproclass entry for uniprot:{UniProtKB_accession}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "iproclass:{UniProtKB_accession}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-uniprot ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "http://bio2rdf.org/uniprot:{UniProtKB_accession}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-uniprot ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/uniprot:{UniProtKB_ID}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-uniprot ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://purl.uniprot.org/uniprot/{UniProtKB_accession}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-ncbigene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info1>;
			rr:joinCondition [ rr:child "EntrezGene"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-refseq ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/refseq:{RefSeq}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-taxon ];
		rr:objectMap	[
			rr:parentTriplesMap <TMtaxonomy-names1>;
			rr:joinCondition [ rr:child "NCBI_taxonomy"; rr:parent "tax_id"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-gi ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/gi:{NCBI_GI_numer}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-pdb ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pdb:{PDB}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-pfam ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pfam:{Pfam}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-go ];
		rr:objectMap	[
			fnml:functionValue [
				rr:predicateObjectMap [
					rr:predicateMap fno:executes ;
					rr:objectMap [ rr:constant sql:concat ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam ;
					rr:objectMap [ rr:constant "http://purl.obolibrary.org/obo/" ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam2 ;
					rr:objectMap	[
						fnml:functionValue [
							rr:predicateObjectMap [
								rr:predicateMap fno:executes ;
								rr:objectMap [ rr:constant sql:replace ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam ;
								rr:objectMap [ rml:reference "GO_ID" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam2 ;
								rr:objectMap [ rr:constant ":" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam3 ;
								rr:objectMap [ rr:constant "_" ];
							];
						];
					];
				];
			];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-go ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/go:{GP}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-pirsf ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pirsf:{PIRSF}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-ipi ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/ipi:{IPI}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:seeAlso ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://uniprot.org/uniref/{UniRef100}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:seeAlso ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://uniprot.org/uniref/{UniRef90}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:seeAlso ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://uniprot.org/uniref/{UniRef50}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-uniparc ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/uniparc:{UniParc}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:seeAlso ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://uniprot.org/uniparc:{UniParc}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-omim ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/omim:{MIM}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-unigene ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/unigene:{UniGene}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:ensembl ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/ensembl:{Ensembl}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:pubmed ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pubmed:{PubMed_ID}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-genbank ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/genbank:{EMBL/GenBank/DDBJ}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ipcv:x-genbank ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/genbank:{EMBL_protein_id}"; ];
	];

.

