@prefix ncbiv: <http://bio2rdf.org/ncbigene_vocabulary:> .

<TMncbigene-gene2unigene1>

	rml:logicalSource [
		rml:source "ftp://ftp.ncbi.nih.gov/gene/DATA/gene2unigene" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ncbiv:Resource;
		rr:template "http://bio2rdf.org/ncbigene:{GeneID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ncbiv:x-unigene ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "UniGene_cluster"; rr:datatype xsd:string; ];
	];

.

