	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "[ncbigene_resource:{genomic_nucleotide_accession.version}/{start_position_on_the_genomic_accession}-{end_position_on_the_genomic_accession}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant faldo:begin ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene2accession3>;
			rr:joinCondition [ rr:child "{genomic_nucleotide_accession.version}/{start_position_on_the_genomic_accession}"; rr:parent "{genomic_nucleotide_accession.version}/{start_position_on_the_genomic_accession}"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant faldo:end ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene2accession4>;
			rr:joinCondition [ rr:child "{genomic_nucleotide_accession.version}/{end_position_on_the_genomic_accession}"; rr:parent "{genomic_nucleotide_accession.version}/{end_position_on_the_genomic_accession}"; ];
		];
	];

.


