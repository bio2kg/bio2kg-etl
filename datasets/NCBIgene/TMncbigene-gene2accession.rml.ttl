<TMncbigene-gene2accession1>

	rml:logicalSource [
		rml:source "ftp://ftp.ncbi.nih.gov/gene/DATA/gene2accession.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ncbiv:Resource;
		rr:template "http://bio2rdf.org/ncbigene:{GeneID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant faldo:location ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene2accession2>;
			rr:joinCondition [ rr:child "{genomic_nucleotide_accession.version}/{start_position_on_the_genomic_accession}-{end_position_on_the_genomic_accession}"; rr:parent "{genomic_nucleotide_accession.version}/{start_position_on_the_genomic_accession}-{end_position_on_the_genomic_accession}"; ];
		];
	];

.


