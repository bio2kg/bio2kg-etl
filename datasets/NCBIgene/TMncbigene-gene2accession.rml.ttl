		rr:objectMap	[ rr:termType rr:Literal; rr:template "location of ncbigene:{GeneID} on {genomic_nucleotide_accession.version} [ncbigene_resource:{genomic_nucleotide_accession.version}/{start_position_on_the_genomic_accession}-{end_position_on_the_genomic_accession}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
