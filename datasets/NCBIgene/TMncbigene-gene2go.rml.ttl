@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix ncbiv: <http://bio2rdf.org/ncbigene_vocabulary:> .

<TMncbigene-gene2go1>

	rml:logicalSource [
		rml:source "ftp://ftp.ncbi.nih.gov/gene/DATA/gene2go.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ncbivv:Resource;
		rr:template "http://bio2rdf.org/ncbigene:{GeneID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:template "ncbiv:{Category}" ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "GO_ID"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:template "ncbiv:gene-{Category}-association" ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene2go2>;
			rr:joinCondition [ rr:child "{GeneID}_{GO_ID}"; rr:parent "{GeneID}_{GO_ID}"; ];
		];
	];

.


<TMncbigene-gene2go2>

	rml:logicalSource [
		rml:source "ftp://ftp.ncbi.nih.gov/gene/DATA/gene2go.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ncbivv:Resource;
		rr:template "http://bio2rdf.org/ncbigene_resource:{GeneID}_{GO_ID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "association between {GeneID} and {GO_ID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "ncbigene_resource:{GeneID}_{GO_ID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ncbiv:evidence ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/eco:{Evidence}"; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ncbiv:gene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info1>;
			rr:joinCondition [ rr:child "GeneID"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ncbiv:go-category ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "ncbigene_vocabulary:{Category}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ncbiv:go-term ];
		rr:objectMap	[ rr:termType rr:Literal; rml:reference "GO_ID"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ncbiv:go-term ];
		rr:objectMap	[
			fnml:functionValue [
				rr:predicateObjectMap [
					rr:predicateMap fno:executes ;
					rr:objectMap [ rr:constant sql:concat ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam ;
					rr:objectMap [ rr:constant "http://purl.obolibrary.org/obo/" ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam2 ;
					rr:objectMap	[
						fnml:functionValue [
							rr:predicateObjectMap [
								rr:predicateMap fno:executes ;
								rr:objectMap [ rr:constant sql:replace ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam ;
								rr:objectMap [ rml:reference "GO_ID" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam2 ;
								rr:objectMap [ rr:constant ":" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam3 ;
								rr:objectMap [ rr:constant "_" ];
							];
						];
					];
				];
			];
		];
	];

.

