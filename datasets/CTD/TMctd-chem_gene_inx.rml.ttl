@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix ctdv: <http://bio2rdf.org/ctd_vocabulary:> .

<TMctd-chem_gene_inx1>

	rml:logicalSource [
		rml:source "http://ctdbase.org/reports/CTD_chemicals.tsv.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ctdv:Resource;
		rr:class ctdv:Chemical-Gene-Association;
		rr:template "http://bio2rdf.org/ctd_resource:{ChemicalID}_{GeneID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Association between {GeneSymbol} (ncbigene:{GenID}) and {ChemicalName} (mesh:{ChemicalID}) [ctd_resource:{ChemicalID}{GeneID}]"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Association between {GeneSymbol} (ncbigene:{GenID}) and {ChemicalName} (mesh:{ChemicalID})"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "ctd_resource:{ChemicalID}{GeneID}"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:comment ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Interaction"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:organism ];
		rr:objectMap	[
			rr:parentTriplesMap <TMtaxonomy-names1>;
			rr:joinCondition [ rr:child "Organism"; rr:parent "tax_id"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:gene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info1>;
			rr:joinCondition [ rr:child "GeneID"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:chemical ];
		rr:objectMap	[
			rr:parentTriplesMap <TMctd-chemicals1>;
			rr:joinCondition [ rr:child "MESH:{ChemicalID}"; rr:parent "ChemicalID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:article ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pubmed:{PubMedIDs}"; ];
	];

.

