@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fno: <http://w3id.org/function/ontology#> .
@prefix sql: <http://dchaves.oeg-upm.net/resources/function/sql.ttl#> .
@prefix bv: <http://bio2rdf.org/bio2rdf_vocabulary:> .
@prefix ctdv: <http://bio2rdf.org/ctd_vocabulary:> .

<TMctd-genes_diseases1>

	rml:logicalSource [
		rml:source "http://ctdbase.org/reports/CTD_genes_diseases.tsv.gz" ;
		rml:referenceFormulation ql:CSV ;
	];

	rr:subjectMap [
		a rr:Subject;
		rr:termType rr:IRI;
		rr:class ctdv:Resource;
		rr:class ctdv:Chemical-Disease-Association;
		rr:template "http://bio2rdf.org/ctd_resource:{GeneID}_{DiseaseID}";
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant rdfs:label ];
		rr:objectMap	[
			fnml:functionValue [
				rr:predicateObjectMap [
					rr:predicateMap fno:executes ;
					rr:objectMap [ rr:constant sql:concat ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam ;
					rr:objectMap [ rr:constant "Association between {GeneSymbol} ({GeneID}) - {DiseaseName} ({DiseaseID}) association [ctd_resource:{GeneID}" ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam2 ;
					rr:objectMap	[
						fnml:functionValue [
							rr:predicateObjectMap [
								rr:predicateMap fno:executes ;
								rr:objectMap [ rr:constant sql:regexp_like ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam ;
								rr:objectMap [ rml:reference "DiseaseID" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam2 ;
								rr:objectMap [ rr:constant "[^:]*$" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam3 ;
								rr:objectMap [ rr:constant "c" ];
							];
						];
					];
				];
			];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:title ];
		rr:objectMap	[ rr:termType rr:Literal; rr:template "Association between {GeneSymbol} ({GeneID}) - {DiseaseName} ({DiseaseID}) association"; rr:datatype xsd:string; ];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant dcterms:identifier ];
		rr:objectMap	[
			fnml:functionValue [
				rr:predicateObjectMap [
					rr:predicateMap fno:executes ;
					rr:objectMap [ rr:constant sql:concat ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam ;
					rr:objectMap [ rr:constant "ctd_resource:{GeneID}" ];
				];
				rr:predicateObjectMap [
					rr:predicateMap sql:valueParam2 ;
					rr:objectMap	[
						fnml:functionValue [
							rr:predicateObjectMap [
								rr:predicateMap fno:executes ;
								rr:objectMap [ rr:constant sql:regexp_like ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam ;
								rr:objectMap [ rml:reference "DiseaseID" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam2 ;
								rr:objectMap [ rr:constant "[^:]*$" ];
							];
							rr:predicateObjectMap [
								rr:predicateMap sql:valueParam3 ;
								rr:objectMap [ rr:constant "c" ];
							];
						];
					];
				];
			];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:disease ];
		rr:objectMap	[
			rr:parentTriplesMap <TMctd-diseases1>;
			rr:joinCondition [ rr:child "OmimIDs"; rr:parent "AltDiseaseIDs"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:disease ];
		rr:objectMap	[
			rr:parentTriplesMap <TMctd-diseases1>;
			rr:joinCondition [ rr:child "DiseaseID"; rr:parent "DiseaseID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:gene ];
		rr:objectMap	[
			rr:parentTriplesMap <TMncbigene-gene_info1>;
			rr:joinCondition [ rr:child "GeneID"; rr:parent "GeneID"; ];
		];
	];

	rr:predicateObjectMap [
		rr:predicateMap	[ rr:constant ctdv:article ];
		rr:objectMap	[ rr:termType rr:IRI; rr:template "http://bio2rdf.org/pubmed:{PubMedIDs}"; ];
	];

.

